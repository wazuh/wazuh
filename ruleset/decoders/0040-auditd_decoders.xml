  <parent>auditd</parent>
  <prematch offset="after_parent">^MAC_STATUS </prematch>
  <regex offset="after_parent">^(MAC_STATUS) msg=audit\(\d\d\d\d\d\d\d\d\d\d.\d\d\d:(\d+)\): </regex>
