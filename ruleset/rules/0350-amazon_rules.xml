        <group>aws_trusted_advisor,</group>
        <options>no_full_log</options>
    </rule>

    <rule id="80483" level="3">
        <if_sid>80480</if_sid>
        <field name="aws.status">OK</field>
        <description>AWS Trusted Advisor - [$(aws.uuid)] [$(aws.check-name)]: $(aws.status)</description>
        <group>aws_trusted_advisor,</group>
        <options>no_full_log</options>
