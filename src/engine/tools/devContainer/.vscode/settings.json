        "valarray": "cpp",
        "variant": "cpp",
        "*.inc": "cpp",
        "typeindex": "cpp"
    },
    "yaml.schemas": {
        "${env:ENGINE_SRC}/ruleset/schemas/wazuh-asset.json": [
            "*/decoders/*.yml",
            "*/decoders/*/*.yml",
            "*/decoders/*/*/*.yml",
            "*/decoders/*/*/*/*.yml",
            "*/rules/*.yml",
            "*/rules/*/*.yml",
            "*/rules/*/*/*.yml",
            "*/rules/*/*/*/*.yml",
            "*/outputs/*.yml",
            "*/outputs/*/*.yml",
            "*/outputs/*/*/*.yml",
