{
    "autopep8.args": [
        "--max-line-length",
        "119"
    ],
    "C_Cpp.formatting": "clangFormat",
    "cmake.buildDirectory": "${env:ENGINE_BUILD}",
    "cmake.configureOnOpen": true,
    "cmake.ctest.parallelJobs": 0,
    "cmake.options.statusBarVisibility": "icon",
    "cmake.sourceDirectory": "${env:WAZUH_REPO}/src",
    "editor.rulers": [
        80,
        90,
        120
    ],
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "*.inc": "cpp",
        "*.tcc": "cpp",
        "algorithm": "cpp",
        "any": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "barrier": "cpp",
        "bit": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "cerrno": "c",
        "cfenv": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "cinttypes": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "compare": "cpp",
        "complex": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "coroutine": "cpp",
        "csetjmp": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cuchar": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "exception": "cpp",
        "format": "cpp",
        "forward_list": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "future": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "latch": "cpp",
        "limits": "cpp",
        "list": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "mutex": "cpp",
        "netfwd": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "propagate_const": "cpp",
        "random": "cpp",
        "ranges": "cpp",
        "ratio": "cpp",
        "regex": "cpp",
        "rope": "cpp",
        "scoped_allocator": "cpp",
        "semaphore": "cpp",
        "set": "cpp",
        "shared_mutex": "cpp",
        "slist": "cpp",
        "source_location": "cpp",
        "span": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "stdfloat": "cpp",
        "stop_token": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "syncstream": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "usage": "cpp",
        "utility": "cpp",
        "valarray": "cpp",
        "variant": "cpp",
        "vector": "cpp",
        "hash_map": "cpp",
        "expected": "cpp",
        "generator": "cpp",
        "print": "cpp",
        "spanstream": "cpp",
        "stacktrace": "cpp",
        "text_encoding": "cpp",
        "*.def": "cpp",
        "fft": "cpp",
        "strstream": "cpp",
        "filesystem": "cpp",
        "core": "cpp",
        "*.evaluator": "cpp",
        "alignedvector3": "cpp",
        "bvh": "cpp",
        "nonlinearoptimization": "cpp"
    },
    "files.watcherExclude": {
        "*.gcda": true,
        "*.gcno": true,
        "*.o": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "src/documentation/**": true
    },
    "python.analysis.include": [
        "${env:ENGINE_SRC}/tools/api-communication/src",
        "${env:ENGINE_SRC}/tools/engine-suite/src",
        "${env:ENGINE_SRC}/test/engine-test-utils/src",
        "${env:ENGINE_SRC}/test/helper_tests/engine-helper-test/src",
        "${env:ENGINE_SRC}/test/integration_tests/engine-it/src"
    ],
    "python.defaultInterpreterPath": "${env:TMP_PY_VENV}/bin/python",
    "remote.localPortHost": "allInterfaces",
    "remote.autoForwardPorts": true,
    "testMate.cpp.test.executables": "${env:ENGINE_BUILD}/**/*{test,Test,TEST,_benchmark}*",
    "testMate.cpp.test.workingDirectory": "${env:ENGINE_BUILD}",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#7220a8"
    },
    "yaml.schemas": {
        "wazuh/src/engine/ruleset/schemas/wazuh-decoders.json": [
            "*/decoders/*.yml",
            "*/decoders/*/*.yml",
            "*/decoders/*/*/*.yml",
            "*/decoders/*/*/*/*.yml"
        ],
        "wazuh/src/engine/ruleset/schemas/wazuh-rules.json": [
            "*/rules/*.yml",
            "*/rules/*/*.yml",
            "*/rules/*/*/*.yml",
            "*/rules/*/*/*/*.yml",
        ],
        "wazuh/src/engine/ruleset/schemas/wazuh-filters.json": [
            "*/filters/*.yml",
            "*/filters/*/*.yml",
            "*/filters/*/*/*.yml",
            "*/filters/*/*/*/*.yml"
        ]
    },
    "C_Cpp.clang_format_fallbackStyle": "none",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format-18",
}
