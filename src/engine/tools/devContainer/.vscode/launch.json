        /*********************************************************************************
        *                        C++ Core dump configurations
        *********************************************************************************/
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Open a core dump Wazuh-Engine",
            "program": "${env:ENGINE_BUILD}/main",
            "coreDumpPath": "${input:coreFileName}",
            "cwd": "${env:ENGINE_SRC}",
            "miDebuggerPath": "/usr/bin/gdb",
            "MIMode": "gdb"
        },
        {
            "type": "cppdbg",
            "request": "launch",
