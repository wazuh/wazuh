                                    <xsd:documentation></xsd:documentation>
                              </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CAP_FSETID">
                              <xsd:annotation>
                                    <xsd:documentation></xsd:documentation>
