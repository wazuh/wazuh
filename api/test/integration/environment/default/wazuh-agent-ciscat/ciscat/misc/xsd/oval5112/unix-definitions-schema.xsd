                        <xsd:enumeration value="CAP_NET_BIND_SERVICE">
                              <xsd:annotation>
                                    <xsd:documentation></xsd:documentation>
                              </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CAP_NET_BROADCAST">
                              <xsd:annotation>
                                    <xsd:documentation></xsd:documentation>
