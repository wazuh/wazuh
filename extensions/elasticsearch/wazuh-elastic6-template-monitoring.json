{
  "order": 0,
  "template": "wazuh-monitoring*",
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "wazuh-agent": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "status": {
          "type": "keyword"
        },
        "ip": {
          "type": "keyword"
        },
        "host": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "id": {
          "type": "keyword"
        }
      }
    }
  }
}